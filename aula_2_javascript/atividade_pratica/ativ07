const infoPessoais = {
    nome: "Anakin",
    cpf: "1134456-98",
    cidade: "Imperio",
    notas: [9.0 , 10.0, 8.0, 9.2 ]
};

const somaNotas = infoPessoais.notas.reduce((total, nota)=> total + nota, 0);
const media = somaNotas / infoPessoais.notas.length;
console.log(`\nMedia das Notas: ${media.toFixed(2)}`);

console.log(`\nResumo acadêmico:
    Aluno: ${infoPessoais.nome}
    Cidade: ${infoPessoais.cidade}
    Média: ${media.toFixed(2)}`)